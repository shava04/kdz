<Window x:Class="Football.TeamInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Football"
        mc:Ignorable="d"
        Title="TeamInfo" Height="159.302" Width="300">
    <Grid>
        <Label x:Name="label" Content="Введите команду" HorizontalAlignment="Left" Margin="20,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="152,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Искать инфо" HorizontalAlignment="Left" Margin="20,41,0,0" VerticalAlignment="Top" Width="104" Height="37" FontSize="16" Click="button_Click"/>
        <Button x:Name="button1" Content="Отмена" HorizontalAlignment="Left" Margin="152,41,0,0" VerticalAlignment="Top" Width="120" Height="37" FontSize="20" Click="button1_Click"/>

    </Grid>
</Window>
